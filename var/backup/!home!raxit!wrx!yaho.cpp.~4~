#include <bits/stdc++.h>
using namespace std;

// Shortcuts for "common" data type in contests
typedef long long 		ll;
typedef pair<int,int> 	ii;
typedef vector<int> 	vi;
typedef vector<ii> 		vii;
#define INF 1000000000;
#define REP(i,a,b) for (for i = a; i <= b; i++)

int main () {

  ios::sync_with_stdio(0);
  cin.tie(0);

  int N, T, count;
  vi v;
  cout << "How many numbers: " << "\n";
  cin >> N;

  cout << "Enter your elements here: ";
  for (int i = 0; i < N; ++i) {
    cin >> T;
    v.push_back(T);
  }
  cout << "The contents of the vector are:" << "\n";

  // Walk the vector and read the value using an iterator
  vi :: iterator it = v.begin();

  while (it != v.end()) {
    cout << *it << " ";
    ++it;
  }
  cout << "\n";

  int element;
  cin >> element;
  // Find an element using the `find' algorithm
  it = find(v.begin(), v.end(), element);

  // Checkif value was found
  if (it != v.end()) {
    // Determine position of element using std::distance
    int elementPosition = distance(v.begin(), it);
    cout << "Value " << *it;
    cout << " found in the vector at position: " << elementPosition << "\n";
  }
  return 0;
}
